{
  "extends": "airbnb/base",
  "rules" : {
    "comma-dangle" : 1,
    "no-unused-vars" : 1,
    "object-shorthand" : 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "strict": "off",
    "no-restricted-syntax": [0, "ForInStatement"]
  },
  "globals": {
    "APP_CONFIG": true,
    "describe": true,
    "it": true,
    "global-require": true,
    "beforeEach": true,
    "afterEach": true,
    "browser": true,
    "sinon": true,
    "expect": true
  }
}
